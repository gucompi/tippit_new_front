{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item",
    "description": "Navigation menu items"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "key": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "label": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "path": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "string",
      "required": true
    },
    "order": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "roles": {
      "type": "json",
      "default": []
    }
  }
}

